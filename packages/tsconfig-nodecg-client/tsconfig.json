{
	"extends": "@supportclass/tsconfig-base",
	"compilerOptions": {
		"module": "es2015",
		"experimentalDecorators": true,
		"skipLibCheck": true,
		"paths": {
			"tslib" : ["node_modules/tslib/tslib.d.ts"]
		},

		// Extra output
		"declaration": false,
		"declarationMap": false,
		"sourceMap": false,
		"inlineSourceMap": true
	}
}
